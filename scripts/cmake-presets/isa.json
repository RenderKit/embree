{
  "version": 4,
  "cmakeMinimumRequired": {
    "major": 3,
    "minor": 23,
    "patch": 0
  },
  "configurePresets": [
    {
      "name": "NO_ISA",
      "cacheVariables": {
        "EMBREE_MAX_ISA": "NONE"
      }
    },
    {
      "name": "DEFAULT_ISA",
      "cacheVariables": {
        "EMBREE_MAX_ISA": "Default"
      }
    },
    {
      "name": "SSE2",
      "cacheVariables": {
        "EMBREE_MAX_ISA": "SSE2"
      }
    },
    {
      "name": "SSE4",
      "cacheVariables": {
        "EMBREE_MAX_ISA": "SSE4"
      }
    },
    {
      "name": "AVX",
      "cacheVariables": {
        "EMBREE_MAX_ISA": "AVX"
      }
    },
    {
      "name": "AVX2",
      "cacheVariables": {
        "EMBREE_MAX_ISA": "AVX2"
      }
    },
    {
      "name": "AVX512",
      "cacheVariables": {
        "EMBREE_MAX_ISA": "AVX512"
      }
    },
    {
      "name": "NEON",
      "cacheVariables": {
        "EMBREE_MAX_ISA": "NEON"
      }
    },
    {
      "name": "NEON2X",
      "cacheVariables": {
        "EMBREE_MAX_ISA": "NEON2X"
      }
    },
    {
      "name": "simd128",
      "cacheVariables": {
        "EMBREE_CONFIG": "frequency_level=simd128"
      }
    },
    {
      "name": "simd256",
      "cacheVariables": {
        "EMBREE_CONFIG": "frequency_level=simd256"
      }
    }
  ]
}
